{
  "name": "hdb-database-scripts",
  "version": "1.0.0",
  "description": "Database setup and data import scripts for HDB Smart Analytics",
  "main": "importData.js",
  "scripts": {
    "setup:postgres": "echo 'Run schema.sql in your PostgreSQL client'",
    "setup:mongodb": "node ../mongodb/mongodb-schema.js",
    "import": "node importData.js",
    "test:postgres": "node test-postgres.js",
    "test:mongodb": "node test-mongodb.js",
    "test:all": "npm run test:postgres && npm run test:mongodb"
  },
  "keywords": [
    "postgresql",
    "mongodb",
    "csv",
    "import",
    "postgis"
  ],
  "author": "HDB Analytics Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "mongodb": "^6.20.0",
    "mongoose": "^8.0.3",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
